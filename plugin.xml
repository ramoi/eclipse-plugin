<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
         
        <command
         id="ramoi.toCamel"
         name="to Camel">
         </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
	   <handler
			 class="ramoi.util.handlers.ToCamelHandler"
			 commandId="ramoi.toCamel">
	   </handler>         
   </extension>
   <extension
     point="org.eclipse.ui.bindings">
   <key
         commandId="ramoi.toCamel"
         contextId="org.eclipse.ui.contexts.window"
         schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
         sequence="M3+C">
   </key>
</extension>

   <extension
         point="org.eclipse.ui.commands">
         
        <command
         id="ramoi.toSnake"
         name="to Snake">
         </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
	   <handler
			 class="ramoi.util.handlers.ToSnakeHandler"
			 commandId="ramoi.toSnake">
	   </handler>         
   </extension>
   <extension
     point="org.eclipse.ui.bindings">
	   <key
	         commandId="ramoi.toSnake"
	         contextId="org.eclipse.ui.contexts.window"
	         schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
	         sequence="M3+S">
	   </key>
	</extension>
</plugin>
